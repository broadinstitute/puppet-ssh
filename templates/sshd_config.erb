LogLevel <%= @log_level.upcase %>
<% Array(@acceptenv).each do |a| -%>
AcceptEnv <%= a %>
<% end -%>

<%-
    @valid_keywords.map do |kw|
      next if scope[kw.downcase] == :undef
      next if scope[kw.downcase] == nil

      key = kw
      value = Array(scope[kw.downcase]).join(',')
-%>
<%= [key,value].join(' ') %>
<%-
    end
-%>
