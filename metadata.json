{
  "name": "zleslie-ssh",
  "version": "0.1.2",
  "author": "zleslie",
  "summary": "Puppet ssh management module",
  "license": "Apache-2.0",
  "source": "git://github.com/xaque208/puppet-ssh.git",
  "project_page": "https://github.com/xaque208/puppet-ssh",
  "issues_url": "https://github.com/xaque208/puppet-ssh/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "5.6",
        "5.7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "description": "A Puppet module to configure OpenSSH servers",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    }
  ]
}
